<?xml version="1.0" encoding="utf-8" ?><!--android.support.v7.widget.CardView-->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:clickable="true"
    android:orientation="vertical">


    <LinearLayout
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/squere_edittext"
        android:orientation="horizontal">

        <com.common.view.CTextView
            android:id="@+id/tvTcketName"
            style="@style/txt_000_12"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Bella" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/gray_light_" />

        <com.common.view.CTextView
            android:id="@+id/tvScannerId"
            style="@style/txt_000_12"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Scanner Id" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/gray_light_" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <com.common.view.CTextView
                android:id="@+id/tvIssuedDate"
                style="@style/txt_000_12"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="16/11/2017" />

            <com.common.view.CTextView
                android:id="@+id/tvIssuedTime"
                style="@style/txt_000_12"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="05:30" />

        </LinearLayout>


        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/gray_light_" />


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <com.common.view.CTextView
                android:id="@+id/tvExpiresDate"
                style="@style/txt_000_12"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="16/11/218" />

            <com.common.view.CTextView
                android:id="@+id/tvExpiresTime"
                style="@style/txt_000_12"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="05:30" />

        </LinearLayout>


        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/gray_light_" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical">

            <com.common.view.CTextView
                android:id="@+id/tvRedeemedDate"
                style="@style/txt_000_12"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="16/11/218" />

            <com.common.view.CTextView
                android:id="@+id/tvRedeemedTime"
                style="@style/txt_000_12"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="05:30" />

        </LinearLayout>

        <!--<com.common.view.CTextView-->
        <!--android:padding="1dp"-->
        <!--android:id="@+id/tvRedeemedOn"-->
        <!--style="@style/txt_000_12"-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:gravity="center"-->
        <!--android:text="Redeemed On"-->
        <!--/>-->

    </LinearLayout>


</LinearLayout>

